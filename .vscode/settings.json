{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.associations": {
    "**/package.json": "json",
    "**/*.json": "jsonc"
  },
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/lib": true,
    "**/lib-amd": true,
    "**/test/**/temp": false,
    "**/temp": true,
    "**/coverage": true,
    "**/private": true
  }
}